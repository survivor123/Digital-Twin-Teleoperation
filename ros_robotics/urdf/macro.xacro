<?xml version="1.0"?>  
<robot name="rrbot" xmlns:xacro="http://www.ros.org/wiki/xacro">


  <macro name="default_inert" params="z_value i_value mass">
    <inertial>
      <origin xyz="0 0 ${z_value}" rpy="0 0 0"/>
      <mass value="${mass}" />
      <inertia ixx="${i_value}" ixy="0.0" ixz="0.0"
               iyy="${i_value}" iyz="0.0"
               izz="${i_value}" />
      </inertial>
  </macro>

</robot>
